generator client {
  provider      = "prisma-client"
  binaryTargets = ["native", "rhel-openssl-3.0.x", "linux-musl"]
  output        = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id          Int    @id @default(autoincrement())
  originalURL String
  shortURL    String @unique
  generatedID String @unique
}
